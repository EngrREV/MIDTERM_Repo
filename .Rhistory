grayscale1 <- function(img, x1){
if(x1== TRUE){
colorMode(img) = Grayscale
display(img, all=TRUE)
}
else if(x1 == FALSE){
colorMode(img) = Color
display(img, all=TRUE)
}
else{
print("Input should only be TRUE or FALSE")
}
}
